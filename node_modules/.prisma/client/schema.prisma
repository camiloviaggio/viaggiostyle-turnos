generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Turno {
  id         Int      @id @default(autoincrement())
  nombre     String
  servicio   String
  fecha      DateTime
  hora       String
  creado_en  DateTime @default(now())
  disponible Boolean  @default(true)
}
